{
  "resolveAlias": {
    "@/*": "/*"
  },
  "pages": [
    "pages/TB-home/home",
    "pages/TB-video/video",
    "pages/TB-personal-center/personal-center",
    "packages/package-video-center/pages/video-player/video-player"
  ],
  "subPackages": [
    {
      "root": "packages/package-home-center",
      "name": "home-center",
      "pages": [
        "pages/recommended-daily/recommended-daily",
        "pages/playlist-display/playlist-display",
        "pages/playlist-center/playlist-center",
        "pages/playlist-tags/playlist-tags",
        "pages/toplist-center/toplist-center",
        "pages/recommended-playlist/recommended-playlist",
        "pages/broadcaster-center/broadcaster-center",
        "pages/broadcaster-display/broadcaster-display",
        "pages/broadcaster-tags/broadcaster-tags"
      ]
    },
    {
      "root": "packages/package-sys",
      "name": "sys",
      "pages": [
        "pages/login/login"
      ]
    },
    {
      "root": "packages/package-personal-center",
      "name": "personal-center",
      "pages": [
        "pages/recent-play/recent-play",
        "pages/my-collection/my-collection",
        "pages/mylike-video/mylike-video",
        "pages/edit-info/edit-info"
      ]
    }
  ],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/TB-home/home"
      },
      {
        "pagePath": "pages/TB-video/video"
      },
      {
        "pagePath": "pages/TB-personal-center/personal-center"
      }
    ]
  },
  "usingComponents": {
    "t-avatar": "tdesign-miniprogram/avatar/avatar",
    "t-button": "tdesign-miniprogram/button/button",
    "t-popup": "tdesign-miniprogram/popup/popup",
    "t-message": "tdesign-miniprogram/message/message",
    "t-tabs": "tdesign-miniprogram/tabs/tabs",
    "t-tab-panel": "tdesign-miniprogram/tab-panel/tab-panel",
    "t-upload": "tdesign-miniprogram/upload/upload",
    "t-input": "tdesign-miniprogram/input/input",
    "t-radio": "tdesign-miniprogram/radio/radio",
    "t-radio-group": "tdesign-miniprogram/radio-group/radio-group",
    "t-date-time-picker": "tdesign-miniprogram/date-time-picker/date-time-picker",
    "t-picker": "tdesign-miniprogram/picker/picker",
    "t-picker-item": "tdesign-miniprogram/picker-item/picker-item",
    "t-textarea": "tdesign-miniprogram/textarea/textarea",
    "t-notice-bar": "tdesign-miniprogram/notice-bar/notice-bar",
    "t-dialog": "tdesign-miniprogram/dialog/dialog",
    "t-slider": "tdesign-miniprogram/slider/slider",
    "t-collapse": "tdesign-miniprogram/collapse/collapse",
    "t-collapse-panel": "tdesign-miniprogram/collapse-panel/collapse-panel",
    "info-item-card": "/components/common/info-item-card/info-item-card",
    "song-list": "/components/common/song-list/song-list",
    "music-player": "/components/common/music-player/music-player",
    "touch-panel": "/components/common/touch-panel/touch-panel",
    "play-controll": "/components/common/play-controll/play-controll",
    "song-display-list": "/components/common/song-display-list/song-display-list"
  },
  "window": {
    "navigationBarTitleText": "云音乐",
    "backgroundColor": "#fff",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTextStyle": "black"
  },
  "requireBackgroundModes": [
    "audio"
  ],
  "lazyCodeLoading": "requiredComponents",
  "sitemapLocation": "sitemap.json"
}