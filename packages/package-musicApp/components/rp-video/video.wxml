<!-- 引入模板 -->
<import src="/packages/package-musicApp/template/rp-item-card/item-card"></import>
<scroll-view scroll-y class="rp-video-container">
  <template is='rp-item-card' wx:for="{{videoInfo.list}}" wx:key="index" data="{{...module.getData(item)}}"></template>
</scroll-view>
<wxs module="module">
  // 引入过滤器
  var filter = require('../../../../utils/filter-wxs/filter.wxs')
  module.exports = {
    getData: function (item) {
      return {
        w: '160rpx',
        cover: item.data.coverUrl,
        name: item.data.title,
        infoL: filter.formatTime(item.data.duration) + ' by ' + item.data.creator.nickname,
        infoR: filter.fromNow(item.playTime)
      }
    }
  }
</wxs>